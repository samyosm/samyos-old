{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "outputDirectory": ".vercel/output/static",
  "installCommand": "pnpm install",
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ]
}
